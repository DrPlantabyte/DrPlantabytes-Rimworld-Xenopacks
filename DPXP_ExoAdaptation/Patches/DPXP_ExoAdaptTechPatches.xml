	-->
	
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/ResearchProjectDef[defName="Batteries"]/tags</xpath>
		<nomatch Class="PatchOperationAdd">
			<!-- no <tags> element in this tech def yet -->
			<xpath>/Defs/ResearchProjectDef[defName="Batteries"]</xpath>
			<value>
				<tags>
					<li>DPXP_IndustrialStart</li>
				</tags>
			</value>
		</nomatch>
		<match Class="PatchOperationAdd">
			<!-- <tags> element already present -->
			<xpath>/Defs/ResearchProjectDef[defName="Batteries"]/tags</xpath>
